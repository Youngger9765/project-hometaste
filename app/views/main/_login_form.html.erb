<%= form_for(@user, url: session_path(@user)) do |f| %>
  <div class="field">
    <%= f.email_field :email, autofocus: true, placeholder: "Email" %>
  </div>

  <div class="field">
    <%= f.password_field :password, autocomplete: "off" , placeholder: "Password"%>
  </div>

  <div class="actions">
    <%= f.submit "Log in" , :id => "login_btn"%>
  </div>
<% end %>