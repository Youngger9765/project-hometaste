<%= form_for setup_food(@food), :url => chef_food_path(@chef.id), :method => :patch do |f| %>
    <div class="container">
      <section class="row">
        <div class="col-xs-12 basic_bg_top ui form">
          <div class="row">
            <%= render :partial => 'form', :locals => { :f => f } %>

            <div class="col-xs-12 tac mt15 mt30_sm">
              <%= f.submit 'Upload to menu',:class=>"my_btn", :id=>"chef-submit-btn"%>
            </div>
          </div>
        </div>

      </section>
    </div>
<% end %>


